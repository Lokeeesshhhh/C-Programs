#include<iostream>
using namespace std;
int main()
{
    int x,y,z;
    cout<<"Enter the number x :";
    cin>>x;
    cout<<"Enter the number y :";
    cin>>y;
    cout<<"Enter the number z  :";
    cin>>z;
    if(x>y && x>z)
    {
        
        cout<<"This number is greater :"<<x<<endl;
    }
    
    else if(y>z && y>x)
    {
    
        cout<<"This number is greater :"<<y<<endl;
    
    }

    else{
    
        cout<<"This number is greater :"<<z<<endl;
    }
    
    return 0;
    
    
}
