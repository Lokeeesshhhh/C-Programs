#include <iostream>
using namespace std;
int main() {
    char ch;
    cin >> ch;
    if (ch >= 'A' && ch <= 'Z') 
    {
        cout << "Upper" << endl;
    }
    else if (ch >= 'a' && ch <= 'z')
    {
        cout << "Lower" << endl;
    }
    else if (ch >= '0' && ch <= '9')
    {
        cout << "Number" << endl;
    }
    else 
    {
        cout << "Symbol" << endl;
    }

    return 0;
}
